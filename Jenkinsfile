pipeline {
  agent any

  stages {
    stage ('one') {
      steps {
        sh 'command'
          echo 'running first satge'
      }
    }
    stage ('two') {
      steps {
        sh 'command'
          echo 'running second stage'
      }
    }
    stage ('three') {
      steps {
        sh 'command'
          echo 'running third stage'
      }
    }
  }
}